{
  "name": "jscrambler-webpack-plugin",
  "version": "8.4.25",
  "description": "A webpack plugin to protect your generated bundle using Jscrambler Code Integrity",
  "exports": "./lib/index.js",
  "types": "index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "babel src --out-dir lib",
    "eslint": "eslint src/",
    "eslint:fix": "pnpm run eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jscrambler/jscrambler.git",
    "directory": "packages/jscrambler-webpack-plugin"
  },
  "keywords": [
